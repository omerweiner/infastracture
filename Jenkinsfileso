 
 pipeline {
   triggers {
  pollSCM '* * * * *'
}
  stages {
        stage('SCM') {
            steps {
                 checkout scm
 stage('SonarQube Analysis') {
      def mvn = tool 'module4';
      withSonarQubeEnv() {
        sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=jenkins -Dsonar.projectName='jenkins'"
      }
    }            }
        }
 }       
 
